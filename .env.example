# Von Neu AI Configuration

# Copy this file to .env and add your actual API key
# Never commit .env to version control!

# Llama-Maverick 17B API Configuration
LLAMA_API_KEY=your_api_key_here

# Optional: Alternative model configuration
# LLAMA_MODEL=meta-llama/Llama-3-70b-chat-hf
# LLAMA_API_URL=https://api.deepinfra.com/v1/openai/chat/completions

# Von Neu Chat Settings
VON_NEU_MAX_TOKENS=400
VON_NEU_TEMPERATURE=0.8
VON_NEU_MAX_HISTORY=10

# Rate Limiting (respect API limits)
VON_NEU_RATE_LIMIT=40
VON_NEU_RATE_WINDOW=60